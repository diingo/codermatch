<%= form_for @user do |f| %>
  <%= f.label :firstname %>
  <%= f.text_field :firstname %><br />

  <%= f.label :lastname %>
  <%= f.text_field :lastname %><br />

  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %><br />

  <%= f.submit "MATCH ME!" %>
<% end %>

